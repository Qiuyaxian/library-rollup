{
  "name": "library-rollup",
  "version": "0.0.1",
  "description": "library-rollup中后台组件库",
  "author": "Qiuyaxian",
  "main": "dist/library-rollup.umd.js",
  "module": "dist/library-rollup.esm.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "ISC",
  "private": false,
  "scripts": {
    "clean": "rimraf dist && rimraf docs/**/dist",
    "build": "npm run clean & npm run build:entry & npm run build:esm & npm run build:umd",
    "build:entry": "node build/create-entry.js",
    "build:esm": "rollup --config ./build/rollup.esm.config.js",
    "build:umd": "rollup --config ./build/rollup.umd.config.js",
    "build:analyzer": "cross-env use_analyzer=true npm run build",
    "lint": "npm run lint:eslint & npm run lint:prettier & npm run lint:stylelint",
    "lint:eslint": "eslint . --ext .vue,.js --fix",
    "lint:prettier": "prettier --write --list-different --loglevel warn \"**/*.{js,json,css,less,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix --allow-empty-input \"**/*.{vue,css,scss,sass}\"",
    "serve:docs": "npx vuepress dev docs",
    "build:docs": "vuepress build docs",
    "fileMddir": "npx mddir",
    "test": "jest --config ./jest.config.js --coverage",
    "release": "node scripts/release.js",
    "publish:dev": "node ./scripts/package-publish.js dev",
    "publish:prod": "node ./scripts/package-publish.js prod",
    "commit": "git add . && git-cz",
    "new": "node build/new/index.js",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@commitlint/cz-commitlint": "^17.1.2",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@sixian/css-url": "^1.0.3",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "@vue/babel-preset-jsx": "^1.1.2",
    "@vue/compiler-sfc": "^3.0.0-beta.14",
    "@vue/test-utils": "^1.0.3",
    "autoprefixer": "^10.4.16",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.2.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "commitizen": "^4.2.5",
    "conventional-changelog": "^5.1.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.13",
    "cz-customizable": "^7.0.0",
    "eslint": "^7.2.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.6.0",
    "file-save": "^0.2.0",
    "generate-source-map": "^0.0.5",
    "husky": "^8.0.1",
    "jest": "^29.2.0",
    "jest-environment-jsdom": "^29.1.2",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "lint-staged": "^13.0.3",
    "mddir": "^1.1.1",
    "postcss": "^8.4.18",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^2.7.1",
    "rollup": "^2.35.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-exclude-dependencies-from-bundle": "^1.1.23",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-require-context": "^1.0.1",
    "rollup-plugin-serve": "^2.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-uglify-es": "0.0.1",
    "rollup-plugin-visualizer": "^5.8.2",
    "rollup-plugin-vue": "^5.0.0",
    "sass": "^1.26.5",
    "sass-loader": "^10.0.1",
    "stylelint": "^15.4.0",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-standard": "34",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-scss": "^5.2.1",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.9.7",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@laomao800/vue-demo-box": "^1.1.7",
    "core-js": "^2.6.12",
    "element-ui": "^2.15.14",
    "lodash": "^4.17.21",
    "vue": "^2.6.11"
  },
  "publishConfig": {
    "registry": "xxxx"
  }
}
